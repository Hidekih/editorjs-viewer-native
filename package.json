{
  "name": "editorjs-viewer-native",
  "version": "0.3.1",
  "description": "A React Native viewer for JSON created by EditorJs",
  "main": "dist/index.js",
  "type": "module",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "react-native",
    "ios",
    "android",
    "editorjs",
    "mobile"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hidekih/editorjs-viewer-native.git"
  },
  "author": "Alexandre Hideki Siroma",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf dist && tsup src/index.tsx --format esm --dts --minify --external react,react-native",
    "pub": "tsup src/index.tsx --format esm --dts --minify --external react,react-native && npm publish --access public"
  },
  "dependencies": {
    "html-entities": "^2.3.3"
  },
  "devDependencies": {
    "@types/react": "^18.0.26",
    "@types/react-native": "^0.70.8",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "eslint": "^8.31.0",
    "eslint-plugin-react": "^7.31.11",
    "react": "^18.1.0",
    "react-native": "^0.70.6",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4"
  },
  "bugs": {
    "url": "https://github.com/Hidekih/editorjs-viewer-native/issues"
  },
  "homepage": "https://github.com/Hidekih/editorjs-viewer-native#readme"
}
